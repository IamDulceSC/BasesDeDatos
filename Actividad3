ALTER TABLE direcciones ADD PRIMARY KEY id_direccion;

ALTER TABLE clientes ADD FOREIGN KEY (id_direccion) REFERENCES direcciones;

CREATE VIEW clientes_direcciones AS SELECT id_cliente, nombre, primer_apellido, segundo_apellido, telefono, email, calle, colonia, cp, municipio, estado, pais FROM clientes, direcciones WHERE clientes.id_direccion = direcciones.id_direccion;
